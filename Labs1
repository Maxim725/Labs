1)
#include "pch.h"
#include <iostream>
#define futvkub 7,481;
using namespace std;

int main() {
	setlocale(LC_ALL, "Rus");
	double gal, answer;
	cout << "Введите число галлонов: ";
	cin >> gal;
	answer = gal / futvkub;
	cout << "Эквивалентный объём в кубических футах равен " << answer << endl;
	system("pause");
	return 0;
}
2)
#include "pch.h"
#include <iostream>
using namespace std;

int main(void) {
	cout << 1900 << "   " << 135 << endl
		 << 1991 << "   " << 7290 << endl
		 << 1992 << "   " << 11300 << endl
		 << 1993 << "   " << 16200 << endl;
	system("pause");
}
3)
#include "pch.h"
#include <iostream>
#define Const 10
using namespace std;

int main(void) {
	int value = Const;
	cout << Const << endl;
	value *= 2;
	cout << value << endl;
	value--;
	cout << value << endl;
	system("pause");
}
4)
#include "pch.h"
#include <iostream>
#define Const 10
using namespace std;

int main(void) {
	setlocale(LC_ALL, "Rus");
	cout << "Мой любимый стих" << endl << endl;
	cout << "Ночь, улица, фонарь, аптека, "<< endl;
	cout << "Бессмысленный и тусклый свет. "<< endl;
	cout << "Живи еще хоть четверть века - "<< endl;
	cout << "Все будет так.Исхода нет. "<< endl << endl;

	cout << "Умрешь - начнешь опять сначала "<< endl;
	cout << "И повторится все, как встарь : "<< endl;
	cout << "Ночь, ледяная рябь канала, "<< endl;
	cout << "Аптека, улица, фонарь. "<< endl << endl << endl;
	system("pause");
}
5)

#include "pch.h"
#include <iostream>
using namespace std;

int main(void) {
	setlocale(LC_ALL, "Rus");
	char symbol;
	cout << "Введите символ: ";
	cin >> symbol;
	cout << islower(symbol) << endl;
	system("pause");
}
6) 
#include "pch.h"
#include <iostream>
using namespace std;

int main(void) {
	setlocale(LC_ALL, "Rus");
	int dollars;
	const float GBP = 1.487; //фунт стерлингов
	const float franc = 0.172; //франк
	const float GM = 0.584; //немецкая марка
	const float Jyen = 0.00955; //японская йена
	cout << "Введите количество долларов: ";
	cin >> dollars;
	cout << "Эквивалентное количество долларов в других валютах:" << endl;
	cout << "Фунт стерлингов: " << (float)dollars / GBP << endl;
	cout << "Франк: " << (float)dollars / franc << endl;
	cout << "Немецкая марка: " << (float)dollars / GM << endl;
	cout << "Японская марка: " << (float)dollars / Jyen << endl << endl;
	system("pause");
}
7)
#include "pch.h"
#include <iostream>
using namespace std;

int main(void) {
	setlocale(LC_ALL, "Rus");
	float C;
	cout << "Введите температуру (в цельсиях): ";
	cin >> C;
	float fahrenheit = C * (9 / 5) + 32;
	cout << "Температура по фаренгейту: " << fahrenheit << endl << endl;
}
8)
#include "pch.h"
#include <iostream>
#include <iomanip>
using namespace std;

int main(void) {
	setlocale(LC_ALL, "Rus");
		cout << 1990 << setfill('.') << setw(8) << 135 << endl
			<< 1991 << setfill('.') << setw(8) << 7290 << endl
			<< 1992 << setfill('.') << setw(8) << 11300 << endl
			<< 1993 << setfill('.') << setw(8) << 16200 << endl;
		system("pause");
}
9)
#include "pch.h"
#include <iostream>
#include <iomanip>
using namespace std;

int main(void) {
	setlocale(LC_ALL, "Rus");
	int a, b, c, d;
	cout << "Введите a b c d (формула a/b + c/d):" << endl;
	cout << "a ="; cin >> a;
	cout << "b ="; cin >> b;
	cout << "c ="; cin >> c;
	cout << "d ="; cin >> d;
	cout << a << "/" << b << " + " << c << "/" << d <<" = ";
	cout << "(" << a << "*" << d << " + " << c << "*" << b << ")/(" << b << "*" << d << ") =";
	a *= d;
	c *= b;
	b *= d;
	cout << a + c << "/" << b << endl;
	system("pause");
}
10) ошибкА
#include "pch.h"
#include <iostream>
#include <iomanip>
using namespace std;

int main(void) {
	setlocale(LC_ALL, "Rus");
	int funt, shilling, penc;
	cout << " Введите фунты, шиллинги и пенсы (старый формат):" << endl;
	cin >> funt >> shilling >> penc;
	cout << '\156' << funt << "." << shilling << "." << penc;
	float newfunt;
	newfunt = (float)((shilling * 13) + penc) / 100;
	cout.precision(2);
	cout << newfunt;
	system("pause");
}
11)
#include "pch.h"
#include <iostream>
#include <iomanip>
#include <Windows.h>
using namespace std;

int main(void) {
	setlocale(LC_ALL, "Rus");
	setiosflags(ios::left);
	cout << setiosflags(ios::left) << "Фамилия" << setw(15) << "Имя" << setw(25) << "Адрес" << setw(35) << "Город" << endl
		 << "Петров" << setw(15) << "Василий" << setw(25) << "Кленовая16" << setw(35) << "Санкт-Петербург" << endl
		 << "Иванов" << setw(15) << "Сергей" << setw(25) << "Осиновая3" << setw(35) << "Находка" << endl
		 << "Сидоров" << setw(15) << "Иван" << setw(25) << "Берёзовая21" << setw(35) << "Калининград" << endl;
}
